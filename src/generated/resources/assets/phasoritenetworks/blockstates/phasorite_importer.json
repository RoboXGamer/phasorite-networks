{
  "multipart": [
    {
      "apply": {
        "model": "phasoritenetworks:block/phasorite_importer"
      }
    },
    {
      "apply": {
        "model": "phasoritenetworks:block/phasorite_connector"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "phasoritenetworks:block/phasorite_connector",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "phasoritenetworks:block/phasorite_connector",
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "phasoritenetworks:block/phasorite_connector",
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "phasoritenetworks:block/phasorite_connector",
        "x": 270
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "phasoritenetworks:block/phasorite_connector",
        "x": 90
      },
      "when": {
        "down": "true"
      }
    }
  ]
}